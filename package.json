{
  "name": "@giwisoft/ryze-tello-sdk",
  "author": {
    "name": "Giwi"
  },
  "version": "0.0.2",
  "private": false,
  "scripts": {
    "bower": "(cd src/www/ && bower install)",
    "build": "tsc -p . && yarn bower && yarn html",
    "dev": "'ts-node' src/index.ts",
    "start": "yarn build && node dist/index.js",
    "mock": "ts-node src/servers/mock_server.ts",
    "doc": "yarn build && jsdoc dist -c .jsdoc.json",
    "gh": "yarn doc && gh-pages -d docs",
    "html": "cp-cli 'src/www/' 'dist/www/'"
  },
  "files": [
    "./dist/"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "mqtt": "^4.2.6",
    "open": "^8.0.3",
    "stack-trace": "^0.0.10",
    "websocket": "^1.0.33"
  },
  "devDependencies": {
    "@types/node": "14.14.35",
    "@types/stack-trace": "^0.0.29",
    "@types/websocket": "^1.0.2",
    "bower": "^1.8.12",
    "cp-cli": "^2.0.0",
    "gh-pages": "^3.1.0",
    "jsdoc": "^3.6.6",
    "ts-node": "^9.1.1",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^4.2.3"
  }
}
